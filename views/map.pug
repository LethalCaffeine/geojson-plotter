doctype html
html(lang='en')
    head
        #{title}
        meta(charset="utf-8", name="daytona-beach")
        script(src="https://code.jquery.com/jquery-3.5.1.min.js",
                integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=",
                crossorigin="anonymous")
        link(href='https://unpkg.com/leaflet@1.6.0/dist/leaflet.css', rel='stylesheet',
        integrity="sha512-xwE/Az9zrjBIphAcBb3F6JVqxf46+CDLwfLMHloNu6KEQCAWi6HcDUbeOfBIptF7tcCzusKFjFw2yuvEpDL9wQ==",
        crossorigin="")
        script(src="https://unpkg.com/leaflet@1.6.0/dist/leaflet.js",
        integrity="sha512-gZwIG9x3wUXg2hdXF6+rVkLF/0Vi9U8D2Ntg4Ga5I5BZpVkVxlJWbSQtXPSiUTtC0TjtGOmxa1AJPuV0CPthew==",
        crossorigin="")
        
    body
        article 
            p "Volusia County, FL"
        div(id='mapid', style={height: '600px', width: '1000px'})

        form(id='formParcel')
            .form-group
                label(for='parcelId') ParcelId:
                input#parcelId.form-control(type='text', placeholder='Parcel ID', name='parcelId', required='true') 
                button() Get Parcel

        button(id='clearMap') Clear map
        button(id='plotAllParcels') Plot all parcels
        button(id='plotBlockGroups') Plot average household income over/under 50k
        script(src="public/js/loadMap.js")
            
                
            